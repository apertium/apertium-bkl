LEXICON PronounInfl

%<acc%>:%{S%} # ;
%<dat%>:p # ;
%<com%>:bar # ;
%<acc%>%<ref%>:nan # ;
+yan%<neg%>:yan # ;
%<dem%>%<part%>:serem # ;
# ;

! -mena/-rem unclear (TODO)

LEXICON ModifierInfl

%<adj%>:0 # ;
%<adj%>%<int%>:sus # ;
%<adj%>%<advl%>:%{F%}er # ;
%<adj%>+yan%<neg%>:yan # ;
%<adj%>:tena VerbInfl ;

